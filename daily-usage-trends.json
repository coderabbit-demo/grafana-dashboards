			"targets": [
				{
					"datasource": {
						"type": "grafana-postgresql-datasource",
						"uid": "${DS_POSTGRESQL}"
					},
					"editorMode": "code",
					"format": "table",
					"rawQuery": true,
					"rawSql": "WITH first_pr AS (\n  SELECT MIN(pr_created) AS first_pr_date\n  FROM pr_metrics\n  WHERE org_id = '$org_id'\n)\nSELECT\n  DATE_TRUNC('day', sm.created_at) AS \"time\",\n  CASE \n    WHEN SUM(sm.total_suggestions) > 0 \n    THEN ROUND((SUM(sm.accepted_comment_count)::numeric / SUM(sm.total_suggestions)::numeric) * 100, 2)\n    ELSE 0 \n  END AS \"Daily Rate\"\nFROM suggestion_metrics sm\nCROSS JOIN first_pr fp\nWHERE sm.org_id = '$org_id'\n  AND sm.created_at >= fp.first_pr_date\n  AND $__timeFilter(sm.created_at)\nGROUP BY DATE_TRUNC('day', sm.created_at)\nORDER BY \"time\" ASC;",
					"refId": "A"
				},
				{
					"datasource": {
						"type": "grafana-postgresql-datasource",
						"uid": "${DS_POSTGRESQL}"
					},
					"editorMode": "code",
					"format": "table",
					"rawQuery": true,
					"rawSql": "WITH first_pr AS (\n  SELECT MIN(pr_created) AS first_pr_date\n  FROM pr_metrics\n  WHERE org_id = '$org_id'\n),\ndaily_stats AS (\n  SELECT\n    DATE_TRUNC('day', sm.created_at) AS day,\n    SUM(sm.accepted_comment_count) AS accepted,\n    SUM(sm.total_suggestions) AS total\n  FROM suggestion_metrics sm\n  CROSS JOIN first_pr fp\n  WHERE sm.org_id = '$org_id'\n    AND sm.created_at >= fp.first_pr_date\n    AND $__timeFilter(sm.created_at)\n  GROUP BY DATE_TRUNC('day', sm.created_at)\n)\nSELECT\n  day AS \"time\",\n  CASE \n    WHEN SUM(total) OVER (ORDER BY day ROWS BETWEEN 6 PRECEDING AND CURRENT ROW) > 0\n    THEN ROUND(\n      (SUM(accepted) OVER (ORDER BY day ROWS BETWEEN 6 PRECEDING AND CURRENT ROW)::numeric /\n       SUM(total) OVER (ORDER BY day ROWS BETWEEN 6 PRECEDING AND CURRENT ROW)::numeric) * 100, 2)\n    ELSE 0\n  END AS \"7-Day Rolling Avg\"\nFROM daily_stats\nORDER BY \"time\" ASC;",
					"refId": "B"
				}
			],
			"title": "Daily Acceptance Rate",
			"type": "timeseries"
		},
